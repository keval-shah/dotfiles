#JAVA
export JAVA_HOME_6=/usr/lib/jvm/java-6-sun
export JAVA_HOME_5=/usr/lib/jvm/java-5-sun
export JAVA_HOME=$JAVA_HOME_6
PATH=$PATH:$JAVA_HOME/bin
CLASSPATH=$CLASSPATH:$JAVA_HOME/lib

#WEBLOGIC
export MW_HOME=/home/keval/weblogic
export WL_HOME=$MW_HOME/wlserver

PATH=$PATH:~/shellscripts

#ORACLE
export ORACLE_HOME=/usr/lib/oracle/11.2/client64
PATH=$PATH:$ORACLE_HOME/bin
export TNS_ADMIN=$ORACLE_HOME

#OC4J
export OC4J_HOME=/home/keval/frameworki/oc4j/
export OC4J_JVM_ARGS="-Xmx640m -XX:PermSize=64m -XX:MaxPermSize=128m -Dframework.log.dir=$OC4J_HOME/j2ee/home/log"
export ANT_OPTS="-XX:MaxPermSize=256m -Xmx2048m -Denv.ORACLE_HOME=$OC4J_HOME"
